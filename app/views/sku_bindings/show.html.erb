<table class="table table-striped table-bordered">
  <tbody>
    <%- SkuBinding.detail_shown_attributes.each do |attr_name| -%>
      <tr>
        <th width="130"><%= SkuBinding.human_attribute_name(attr_name)%></th>
        <td>
        <%= @sku_binding.send(attr_name) %>
        </td>
      </tr>
    <%- end -%>
  </tbody>
</table>
<div class="form-group">
  <div class="col-sm-offset-2 col-sm-10">
    <%= link_to "返回列表", sku_bindings_path, class: "btn" %>&nbsp; &nbsp; &nbsp;
    <%= link_to "编辑", edit_sku_binding_path(@sku_binding), class: "btn btn-primary" %>
  </div>
</div>